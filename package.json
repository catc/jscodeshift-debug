{
  "name": "jscodeshift-debug",
  "version": "1.0.0",
  "description": "Debug utilities for jscodeshift",
  "main": "dist/debug.js",
  "types": "debug.d.ts",
  "scripts": {
    "build": "yarn tsc --declaration --allowJs --emitDeclarationOnly && yarn tsc --outDir dist",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch"
  },
  "keywords": [
    "jscodeshift",
    "ast",
    "esprima",
    "recast",
    "debug",
    "tree"
  ],
  "author": "catc",
  "license": "MIT",
  "dependencies": {
    "chalk": "4.1.2",
    "jscodeshift": "^0.13.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/jscodeshift": "^0.11.3",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  }
}
